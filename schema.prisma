datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

generator client {
    provider = "go run github.com/steebchen/prisma-client-go"
}

model Circuit {
    id        Int      @id @default(autoincrement())
    name      String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model Sketch {
    id        Int      @id @default(autoincrement())
    name      String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
